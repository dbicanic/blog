<div class="col-sm-10 col-sm-offset-1 col-xs-12">
  <% @posts.each do |post| %>
    <div class="col-md-3 gridBorder">
      <h4><%= post.title %></h4>
      <p><%= post.created_at.strftime('%b %d, %Y') %></p>
      <%= link_to "Show", post_path(post) %>
  	</div>
  <% end %>
  <div class="text-center">
    <%= will_paginate @posts %>
  </div>
</div>
